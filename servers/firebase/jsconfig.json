{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "declaration": false,
    "experimentalDecorators": false,
    "moduleResolution": "node",
    "target": "es2019",
    "module": "es2020",
    "lib": [
      "es2019",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "build-*",
    "data",
    "logs"
  ]
}
